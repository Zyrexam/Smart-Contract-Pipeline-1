{
  "contract_name": "RentalNFTSystem",
  "contract_type": "NFT Rental",
  "description": "A smart contract system that allows users to rent NFTs for a fixed duration.",
  "state_variables": [
    {
      "name": "nftOwner",
      "type": "address",
      "visibility": "public",
      "initial_value": null
    },
    {
      "name": "renter",
      "type": "address",
      "visibility": "public",
      "initial_value": null
    },
    {
      "name": "rentalDuration",
      "type": "uint256",
      "visibility": "public",
      "initial_value": 0
    },
    {
      "name": "rentalStartTime",
      "type": "uint256",
      "visibility": "public",
      "initial_value": 0
    },
    {
      "name": "nftId",
      "type": "uint256",
      "visibility": "public",
      "initial_value": 0
    },
    {
      "name": "isRented",
      "type": "bool",
      "visibility": "public",
      "initial_value": false
    }
  ],
  "roles": [
    {
      "name": "Owner",
      "permissions": [
        "list NFT for rent",
        "withdraw NFT after rental"
      ]
    },
    {
      "name": "Renter",
      "permissions": [
        "rent NFT"
      ]
    }
  ],
  "functions": [
    {
      "name": "listNFTForRent",
      "visibility": "public",
      "inputs": [
        {
          "name": "nftId",
          "type": "uint256"
        },
        {
          "name": "rentalDuration",
          "type": "uint256"
        }
      ],
      "outputs": [],
      "restricted_to": "Owner",
      "description": "Allows the owner to list an NFT for rent with a specified duration."
    },
    {
      "name": "rentNFT",
      "visibility": "public",
      "inputs": [
        {
          "name": "nftId",
          "type": "uint256"
        }
      ],
      "outputs": [],
      "restricted_to": "Renter",
      "description": "Allows a user to rent an NFT for the specified duration."
    },
    {
      "name": "withdrawNFT",
      "visibility": "public",
      "inputs": [
        {
          "name": "nftId",
          "type": "uint256"
        }
      ],
      "outputs": [],
      "restricted_to": "Owner",
      "description": "Allows the owner to withdraw the NFT after the rental period has ended."
    }
  ],
  "events": [
    {
      "name": "NFTListed",
      "parameters": [
        {
          "name": "nftId",
          "type": "uint256"
        },
        {
          "name": "rentalDuration",
          "type": "uint256"
        }
      ]
    },
    {
      "name": "NFTRented",
      "parameters": [
        {
          "name": "nftId",
          "type": "uint256"
        },
        {
          "name": "renter",
          "type": "address"
        }
      ]
    },
    {
      "name": "NFTWithdrawn",
      "parameters": [
        {
          "name": "nftId",
          "type": "uint256"
        }
      ]
    }
  ],
  "modifiers": [
    {
      "name": "onlyOwner",
      "condition": "msg.sender == nftOwner",
      "description": "Ensures that the function is called by the NFT owner."
    },
    {
      "name": "onlyRenter",
      "condition": "msg.sender == renter",
      "description": "Ensures that the function is called by the renter."
    },
    {
      "name": "rentalPeriodEnded",
      "condition": "block.timestamp >= rentalStartTime + rentalDuration",
      "description": "Ensures that the rental period has ended."
    }
  ]
}