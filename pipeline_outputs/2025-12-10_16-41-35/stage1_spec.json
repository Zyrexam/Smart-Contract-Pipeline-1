    {
      "name": "treasuryWallet",
      "type": "address",
      "visibility": "private",
      "initial_value": ""
    },
    {
      "name": "taxRate",
      "type": "uint256",
      "visibility": "private",
      "initial_value": "3"
    }
  ],
  "roles": [],
  "functions": [
    {
      "name": "transfer",
      "visibility": "public",
      "inputs": [
        {
          "name": "to",
          "type": "address"
        },
        {
          "name": "amount",
          "type": "uint256"
        }
      ],
      "outputs": [
        {
          "name": "success",
          "type": "bool"
        }
      ],
      "restricted_to": "",
